{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wptravel/trip-price",
  "version": "0.1.0",
  "title": "Trip Price",
  "category": "wp-travel-single-trip-blocks",
  "icon": "money-alt",
  "description": "Display minimum Price among the prices added for booking of trip.",
  "attributes": {
    "showFromLabel": {
      "type": "boolean",
      "default": true
    },
    "hideRegularPrice": {
      "type": "boolean",
      "default": false
    },
    "textAlign": {
      "type": "string",
      "default": "left"
    },
    "color": {
      "type": "string",
      "default": ""
    },
    "fontSize": {
      "type": "number",
      "default": 22
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "trip-price",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}