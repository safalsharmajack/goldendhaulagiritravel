{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wp-travel-block/slides",
  "title": "Slides",
  "category": "widgets",
  "parent": [
    "wp-travel-block/slider"
  ],
  "description": "Single Slide block to be used with Slider block",
  "supports": {
    "anchor": true,
    "reusable": false,
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "spacing": {
      "padding": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    },
    "__experimentalLayout": true
  },
  "textdomain": "slides",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}