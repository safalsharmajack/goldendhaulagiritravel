{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wptravel/breadcrumb",
  "version": "0.1.0",
  "title": "Breadcrumb",
  "category": "wp-travel-blocks",
  "icon": "ellipsis",
  "description": "Display the path that reveals where the user is on the site.",
  "attributes": {
    "seperator": {
      "type": "string",
      "default": ">"
    },
    "textAlign": {
      "type": "string",
      "default": "left"
    },
    "linkUnderLine": {
      "type": "boolean",
      "default": false
    },
    "textSize": {
      "type": "number",
      "default": 16
    },
    "linkColor": {
      "type": "string",
      "default": ""
    },
    "textColor": {
      "type": "string",
      "default": ""
    },
    "separatorColor": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "breadcrumb",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}