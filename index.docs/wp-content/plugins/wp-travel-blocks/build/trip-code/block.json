{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wptravel/trip-code",
  "version": "0.1.0",
  "title": "Trip Code",
  "category": "wp-travel-single-trip-blocks",
  "icon": "clipboard",
  "description": "Display the unique code added for trip.",
  "usesContext": [
    "postId",
    "postType",
    "queryId"
  ],
  "attributes": {
    "textAlign": {
      "type": "string",
      "default": "left"
    },
    "extraClass": {
      "type": "string",
      "default": ""
    },
    "textColor": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "trip-code",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}